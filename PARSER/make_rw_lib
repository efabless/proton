      my $delay_first_value  = (split(/,/,$cellRiseDelayVal[0]))[0];
      if($delay_first_value ne ""){
        print WRITE_LIB "\tdelay:\t\t$delay_first_value\;\n";
      }
    }elsif($cell =~ /^nand/i){
      my @pins = $TLDB{$cell}->dbTimingLibGetPins;
