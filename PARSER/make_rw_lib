#    elsif($_ =~ m/^\!\s*\(\s*\(\s*$identifier\s*((\|)|(\+))\s*$identifier\s*((\|)|(\+))\s*$identifier\s*\)\s+\(\s*$identifier\s*((\|)|(\+))\s*$identifier\s*\)\s*\)/){
