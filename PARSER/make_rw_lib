#      #print " and or invert";
#      $v = $v . " aoi" ;
#    }
#    elsif($_ =~ m/^\(\s*\!\s*\(\s*\(\s*$identifier $identifier\s*\)\s*((\|)|(\+))\s*\(\s*$identifier $identifier\s*\)\s*((\|)|(\+))\s*\(\s*$identifier $identifier\s*\)\s*\)\s*\)/){
#      #print " and or invert";
#      $v = $v . " aoi" ;
#    }
#    elsif($_ =~ m/^\(\s*\!\s*\(\s*\(\s*$identifier $identifier\s*\)\s*((\|)|(\+))\s*\(\s*$identifier $identifier\s*\)\s*\)\s*\)/){
#      #print " and or invert";
#      $v = $v . " aoi" ;
